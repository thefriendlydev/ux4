.primaryNav
  position: fixed
  width: 100%
  top: 0
  background-image: linear-gradient(-180deg, rgba(0,0,0,0.50) 0%, rgba(0,0,0,0.00) 100%)
  z-index: 5
  padding: 15px 0

.navigation-list
  display: none
  ul
    @extend .plainList
    li a
      color: #FFFFFF

.navLogo
  display: inline-block
  img
    max-height: 30px
    width: auto
